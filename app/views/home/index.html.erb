<% content_for(:navigation_title, t('.navigation_title')) %>
<% content_for(:page_title, t('.page_title')) %>
<div class="col s12">
  <div class="container-embedded">
      <%= render partial: 'tasks/list', locals: { tasks: @tasks } %>
  </div>
</div>
<div class="fixed-action-btn">
  <%= link_to new_task_url, data: { no_turbolink: true }, class: 'btn-floating btn-large waves-effect waves-light task-creation-link' do %>
    <i class="large material-icons">create</i>
  <% end -%>
</div>
